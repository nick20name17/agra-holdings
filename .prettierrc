{
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"semi": false,
	"trailingComma": "none",
	"printWidth": 80,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^components/(.*)$",
		"^providers/(.*)$",
		"^aoi/(.*)$",
		"^assets/(.*)$",
		"^types/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.css)$",
		"(.scss)$"
	]
}
